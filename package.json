{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "starter setup for react-redux with webpack configuration and karma",
  "private": true,
  "scripts": {
    "test": "karma start --single-run --browsers PhantomJS --reporters junit,dots || true",
    "test:browser": "karma start --browsers Chrome",
    "test:watch": "karma start --browsers PhantomJS",
    "lint": "eslint src/** --ext .js  || true",
    "start": "webpack-dev-server --progress",
    "server": "webpack-dev-server --progress",
    "dev": "parallelshell \"npm run test:watch\" \"npm run start\"",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress -p"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "bootstrap": "^3.3.7",
