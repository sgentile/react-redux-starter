    "react-bootstrap": "^0.30.3",
    "react-bootstrap-table": "^2.5.3",
    "react-datetime": "^2.6.0",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5"
  },
